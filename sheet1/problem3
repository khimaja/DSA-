//Missing number in array





// User function template for C++

class Solution{
  public:
    int missingNumber(vector<int>& array, int n) {
        int ans = (n*(n+1))/2;
        
        for(int i=0;i<array.size();i++)
        {
            ans = ans-array[i];
        }
        return ans;
    }
};
